<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bestzmr.buyi.system.mapper.RoleMapper">

    <select id="findById" resultType="com.bestzmr.buyi.system.entity.Role">
        select * from role where id = #{roleId}
    </select>
    <select id="findByName" resultType="com.bestzmr.buyi.system.entity.Role">
        select * from role where name = #{roleName}
    </select>
</mapper>